@import 'nib'
@import 'normalize.styl'
@import 'responsiveslides.styl'
@import 'themes.styl'
@import 'demo.styl'
@import 'animate.styl'
rojo = #C21616
gris_A = #FEFEFF
gris_B = #E6E6E6
gris_C = #7A7A7A
html 
	overflow-y hidden 
@font-face {
    font-family: 'avantgargotitcteeregular';
    src: url('../fonts/ae10013t-webfont.eot');
    src: url('../fonts/ae10013t-webfont.eot?#iefix') format('embedded-opentype'),
         url('../fonts/ae10013t-webfont.woff2') format('woff2'),
         url('../fonts/ae10013t-webfont.woff') format('woff'),
         url('../fonts/ae10013t-webfont.ttf') format('truetype'),
         url('../fonts/ae10013t-webfont.svg#avantgargotitcteeregular') format('svg');
    font-weight: normal;
    font-style: normal;

}
@font-face {
	font-family: 'icomoon';
	src:url('../fonts/icomoon.eot?40n1bh');
	src:url('../fonts/icomoon.eot?#iefix40n1bh') format('embedded-opentype'),
		url('../fonts/icomoon.woff?40n1bh') format('woff'),
		url('../fonts/icomoon.ttf?40n1bh') format('truetype'),
		url('../fonts/icomoon.svg?40n1bh#icomoon') format('svg');
	font-weight: normal;
	font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
	font-family: 'icomoon';
	speak: none;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	line-height: 1;

	/* Better Font Rendering =========== */
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}

.icon-list:before {
	font-size 1.6em
	color white
	text-decoration none
	content: "\e600";
}
body
	
	font-size 16px
	font-family 'avantgargotitcteeregular', sans-serif
	background white

.u-triangle
	position absolute
	margin-left: -15px
	left 50%
	bottom 0px
	width 0px
	border-top 10px solid transparent
	border-left 15px solid transparent
	border-right 15px solid transparent
	border-bottom 10px solid rojo
.Cabecera
	position relative
	background-color #F0F0F0
	&-button
		text-decoration none
		display none
		margin 0.4em 0.1em
		font-size 1.3em
		
	&-social
		text-align right
		&Container
			padding 0.5em 1em 0.5em 0em 
			display inline-block
			margin 0
		&Link
			vertical-align top
	&-logoConteiner
		z-index 3
		top 0
		left 3%
		margin 0
		position absolute
		width 32%
		min-width 298px
	&-logo 
		width 100%
	&-enlaceObra
		text-align center
		text-decoration none
		color white
		display inline-block 
		font-size 1em
		width 95px
		padding-top 25px 
		height 70px
		border-radius 50%
		background  rojo 
		opacity 0.8
		
		&.ejecucion
			background #F58F11
		&:hover
			background-image url('../img/flechaUp.png') 
			background-size 30px
			background-repeat no-repeat
			background-position 50% 4px
			
	&-contenedorObras
		position absolute
		// bottom -90px
		width 100%
		text-align center
	&-navegacion
		
		background rojo	
		height 50px
		&List
			margin 0
			padding 0 6rem
			text-align right
			font-size 0
		&Item
			list-style none
			display inline-block
		&Link
			height: 18px;
			text-align center
			background rojo	
			position relative
			font-size 1rem
			line-height 18px
			color white
			z-index 10
			padding 1em 1em
			text-decoration none
			display block
			font-weight 500
			&:hover
				color black
				font-weight 600
				background white
.active
	color black
	font-weight 600
	background white
.Cuerpo
	
	padding 0.2rem 0 0 0 
	overflow hidden
	font-size 0
	background white
	position relative
	overflow visible
	&-imagenFondoInicio
		position relative
		z-index 0
		& .centered-btns1_tabs
			position: absolute;
			z-index: 6;
			bottom: 75px;
			left:0;
			right: 0;
			margin-top: 10px;
			text-align: center;
	&-imagenComplemento
		z-index 5
		position absolute
		width 15%
		min-width 115px
		top 13rem
		left 1rem

	&-textoComplemento

		text-align right
		width 220px
		position absolute
		color white
		z-index 2
		font-size 1.4rem
		top 2em
		right 2em
		p
			padding 4px 0
			font-weight 100
			color gris_A
			margin 0.4rem 0
			border-bottom 2px solid gris_A
			border-top 2px solid gris_A
	&-imagen
		
		width 52%
		margin 0
		position relative
		overflow hidden
		img 
			min-height 550px	
		&Oblicua
			top 0
			right 0
			position absolute
			border-left 160px solid transparent
			border-bottom 556px solid white
			width 0px
			z-index 2
	&-section,&-imagen
		display inline-block
		vertical-align top
	&-section
		position relative
		z-index 2
		padding 20px
		box-sizing border-box
		margin 0
		width 48%
		text-align center
		font-size 1rem
		margin-bottom 60px
		h1
			padding-left 0em
			margin 1em 0 0 0
			font-weight 100
			font-size 2rem
			text-align left
	&-itemImage
		border-radius 50%
		width 100%
		width 120px
		height 120px
		border 5px solid white
		box-shadow rgba(0,0,0,0.4) -2px -1px 8px
	&-itemConteiner
		vertical-align top
		margin 0.5em 0
		width 175px
		display inline-block
		font-size 1.1rem
		
	&-itemLink
		
		text-decoration none
		font-weight 600
		color gris_C
	&-especialidad
		position relative
	&-maquinariaList
		padding 1em 1em 1em 0em
		margin 0
		&.espaciadoEx
			padding 0.6em 0
	&-maquinariaItem

		background-color gris_B
		background-image url('../img/flechaDer.png'), url('../img/degradado.jpg')
		background-repeat no-repeat, repeat-y
		background-size 25px 26px,1000px 5px
		padding 4px 0 4px 40px
		text-align left
		font-size 1.1rem
		font-weight 900
		list-style none

	&-textList
		font-size 0.9em
		margin 0.3em  0 0 0
		text-align left	
		padding-left 32px
		&:first-child
			padding-bottom 2rem
	&-sectionObras
		padding-top 2rem
		text-align left
		font-size 1rem
		width 55%
		text-align right
		display inline-block
		vertical-align top
	&-itemObras
		vertical-align top
		padding-top 7em
		display inline-block
		width 45%	
		font-size 1rem
		position relative
		z-index 0
.imagenInicio
	min-width 100%
.imagenCompObras
	position absolute
	width 15%
	min-width 115px
	top -3rem
	left 1rem
.listaObras
	margin-left 10%
	margin-top 1.5rem
.ObrasEjecucion
	box-shadow gris_B 3px 3px 8px
	background-color gris_B
	background-image url('../img/flechaDerAma.png')
	background-repeat no-repeat
	background-size 31px 31px
	padding 4px 0 4px 40px
	text-align left
	font-size 1.1rem
	font-weight 900
	list-style none
.obrasLink
	max-width 180px
	border 4px solid gris_B
	display inline-block
	margin 8px
	width 28%
	// margin 0 1%
	box-shadow rgba(0,0,0,0.4) -2px -1px 8px
.obrasImage
	vertical-align top
	// margin 5px
	width 100%
	// border 1px solid white
.engra3
	z-index 1
	max-width 150px
	left auto
	right 1rem
	top 17rem
	
.itemObra 
	margin 1em 0.5em 0 0 
	background-image url('../img/flechaDer2.png'), url('../img/degradado.jpg')
	background-repeat no-repeat, repeat-y
	background-size 28px 26px, 1000px 5px
	padding 4px 0 4px 40px
	text-align left
	font-size 1.1rem
	font-weight 900
	list-style none
.itemObra2
	margin 1em 0.5em 0 0 
	background-color gris_B
	background-image url('../img/flechaDerAma2.png'),url('../img/degradado.jpg')
	background-repeat no-repeat, repeat-y
	background-size 28px 26px, 1000px 5px
	padding 4px 0 4px 40px
	text-align left
	font-size 1.1rem
	font-weight 900
	list-style none
.contactanosForm
	text-align left
.formConteiner
	padding-left 10px
	text-align left
	label
		display inline-block
		width 70px 
.formulario
	border 2px solid #DDDDDD
	width 40%;
	height 18px
	display block;
	margin 0.2rem 0
	padding-left 0.5em
	display inline-block
	&.textarea
		width 80%
		height 80px
		margin-bottom 3rem
.areaTexto
	position relative
	margin-left 74px
.formulario-submit
	font-style italic
	font-weight 900
	left 80%
	top 6rem
	margin-left -50px
	border transparent
	font-size 1.4rem
	background-color transparent
	color rojo
	position absolute
	display block
.obrasTitleE
	padding-right 10px
	margin-right 8px
	margin-top 1rem
	font-size 2rem
	text-align right
	color rojo
	background-image url('../img/degradado2.jpg')
	background-repeat  repeat-y
	background-size 1000px 5px
	background-position right top
	&.enEjecucion
		color #F58F11
.aux-engranajes
	display none
.titleFotos
	padding-left 0em
	margin 0.5em 0 0 0
	font-weight 100
	font-size 1.8rem
	text-align center
.fotosContenedor
	text-align center
.carusel
	min-width 600px
	width 50%
	vertical-align top
	display inline-block
.description
	font-size 1rem
	width 20%
	background-color gris_B
	vertical-align top
	display inline-block
	padding 0.5rem
.Pie
	position fixed
	z-index 6
	bottom 0
	left 0
	right 0
	height 30px
	font-size 0.7em
	background #F0F0F0
	color gris_C
	text-align center
	padding 1em 0
	&-busqueda
		text-align left
		top 1.2em
		right 2em
		position absolute
		&Text
			display block
		&Plus
			margin-left -4px
			border 1px solid rojo
			font-weight 900
			background rojo
			color white
	&-text
		margin 4px
.formText
	margin 0
	color #444
	text-align left
.marcaAgua
	z-index 6
	width 25%
	position absolute
	top 180px
	left 32%
@media screen and (max-width:1180px)
	.Cabecera-navegacionList
		padding 0 0 0 0
@media screen and (max-width:1030px)
	.Cabecera-navegacionList
		position absolute
		z-index 15
		top 96px
		left -148px
		transition all 0.3s ease
	.Cabecera-navegacionItem
		display block
	.Cabecera-button
		float right
		display inline-block
	.is-active
		left 0px
@media screen and (max-width:807px)
	.carusel,.description
		display block 
		width 100%
	.carusel
		min-width 0
	.Cuerpo
		max-height 900px
		overflow visible
@media screen and (max-width:739px)
	.Cuerpo-itemConteiner
		margin 0.8em  0 0 0
	.Cuerpo-itemImage
		border-radius 50%
		width 40px
		height 40px
		border 5px solid white
		box-shadow rgba(0,0,0,0.4) -2px -1px 8px
@media screen and (max-width:600px)
	html 
		overflow-y visible
	.obrasLink
		margin 0
	.Cuerpo-itemObras
		display block
		width 100%
	.Cuerpo-sectionObras
		padding-bottom 60px
		display block
		text-align center
		width 100%
		padding-top 0em
	.listaObras
		margin-left 10%
		width 80%
		padding-left 0
	.Cuerpo
		max-height 900px
		overflow visible
	a.rslides_nav.rslides2_nav.prev,a.rslides_nav.rslides2_nav.next
		display none
	.descripcionFoto
		display none
	.contenedor
		width 100%
@media screen and (max-width:550px)
		.Cuerpo-imagen
			overflow visible
			width auto
			img
				min-height 0px
				width 100%
			overflow visible
		.Cuerpo-section
			width 100%
			position relative
			z-index 4
		.Cuerpo-imagenOblicua
			display none
		.Cuerpo-imagen,.Cuerpo-section
			display block
		.Cuerpo-imagenComplemento
			display none
		.aux-engranajes
			display block
			position absolute
			top 4rem
			right 1rem
			z-index 3
		.Cuerpo
			max-height 900px
			overflow visible
		.centered-btns_tabs
			display none
			
@media screen and (max-width:520px)
	.Cuerpo-textoComplemento p
		margin 5rem 0
	.Cuerpo-section h1
		
		font-weight 900
		text-align center
		font-size 1.2rem
	.Pie-text
		text-align left
		margin-left 10px
@media screen and (max-width:420px)
	.aux-engranajes
		top 8rem
		width 100px
		right 1rem
	.Pie-busquedaText
		opacity 0